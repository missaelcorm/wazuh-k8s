#!/bin/bash

wget https://packages.wazuh.com/4.x/apt/pool/main/w/wazuh-agent/wazuh-agent_4.11.1-1_amd64.deb && sudo WAZUH_MANAGER='${WAZUH_MANAGER}' WAZUH_REGISTRATION_SERVER='${WAZUH_REGISTRATION_SERVER}' WAZUH_REGISTRATION_PASSWORD=$'${WAZUH_REGISTRATION_PASSWORD}' WAZUH_AGENT_NAME='${WAZUH_AGENT_NAME}' dpkg -i ./wazuh-agent_4.11.1-1_amd64.deb

sudo systemctl daemon-reload
sudo systemctl enable wazuh-agent
sudo systemctl start wazuh-agent
